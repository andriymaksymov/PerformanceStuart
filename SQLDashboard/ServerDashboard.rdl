          <DataField>total_io</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Resource_Governor_Total_Read_IO_Latency__ms_">
          <DataField>Resource Governor Total Read IO Latency (ms)</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Resource_Governor_Total_Write_IO_Latency__ms_">
          <DataField>Resource Governor Total Write IO Latency (ms)</DataField>
